{
  "name": "react-gm",
  "version": "2.3.15",
  "description": "",
  "main": "src/index.js",
  "style": "src/index.css",
  "keywords": [
    "react",
    "react-gm",
    "grid",
    "drop",
    "validate",
    "datepicker",
    "react-component"
  ],
  "scripts": {
    "precommit": "eslint ./src/ ./demo/;",
    "pre": "npm install",
    "clear": "rm -rf build && mkdir build;",
    "start": "npm run clear && NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --inline --hot --progress --colors --port 3000",
    "deploy": "npm install;npm run build && npm run build:min",
    "build": "NODE_ENV=production webpack",
    "build:min": "NODE_ENV=production webpack --config webpack.config.min.js",
    "publishpatch": "npm run deploy && git add --all && git commit --amend -m 'c' && npm version patch && git push origin master:master && npm publish --registry='https://registry.npmjs.org' && cnpm sync react-gm && npm version;",
    "gh-pages": "git branch -D gh-pages; git checkout -b gh-pages; webpack --config webpack.config.dev.js; rm -rf .gitignore; git add --all; git commit -m 'page'; git push origin :gh-pages; git push origin HEAD:gh-pages; git checkout master;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gmfe/react-gm.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gmfe/react-gm/issues"
  },
  "homepage": "https://github.com/gmfe/react-gm#readme",
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gm-bootstrap": "^1.1.2",
    "gm-pinyin": "^2.0.0",
    "gm-util": "^2.1.0",
    "husky": "^0.11.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "moment": "^2.14.1",
    "path": "^0.12.7",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "react": "^15.3.0",
    "react-bootstrap": "^0.30.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.1"
  }
}
